<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="431.0" prefWidth="734.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.sp.napomenalka.controller.ControllerMain">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <children>
            <ComboBox fx:id="listOrganization" maxWidth="1.7976931348623157E308" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#orgButton" text="добавить список организаций" />
            <ComboBox fx:id="listType" maxWidth="1.7976931348623157E308" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#typeButton" text="добавить список тип подписки" />
            <HBox spacing="10.0">
               <children>
                  <DatePicker fx:id="dateStart" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                  <DatePicker fx:id="dateInf" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addList" text="добавить" VBox.vgrow="ALWAYS" />
            <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button mnemonicParsing="false" onAction="#remyve" text="удалить" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="отчёт" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
